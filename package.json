{
  "name": "ultrastar",
  "devDependencies": {
    "@ultrastar/ts-config": "workspace:*",
    "prettier": "^3.5.2"
  },
  "packageManager": "bun@1.2.4",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo watch dev",
    "start": "turbo start",
    "db:push": "turbo db:push",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "turbo": "^2.4.4"
  }
}
