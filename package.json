{
  "name": "ultrastar",
  "devDependencies": {
    "@ultrastar/ts-config": "workspace:*",
    "dotenv-cli": "^7.4.2",
    "prettier": "^3.2.5"
  },
  "packageManager": "bun@1.1.10",
  "private": true,
  "scripts": {
    "build": "dotenv -- turbo build",
    "dev": "dotenv -- turbo watch dev",
