{
  "name": "ultrastar",
  "devDependencies": {
    "@ultrastar/ts-config": "workspace:*",
    "prettier": "^3.2.5"
  },
  "packageManager": "bun@1.1.10",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo watch dev",
